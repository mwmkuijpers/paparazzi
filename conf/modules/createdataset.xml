<!DOCTYPE module SYSTEM "module.dtd">

<module name="createdataset">
  <doc>
    <description>Creates dataset by triggering front and bottom camera to take shots and logging ac data</description>
  </doc>
  <header>
    <file name="createdataset.h"/>
  </header>
  <init fun="init_createdataset()"/>
  <periodic fun="createdataset_periodic()" freq="0.5" start="file_logger_start()" stop="file_logger_stop()" autorun="FALSE"/>

  <makefile>
    <file name="createdataset.c"/>
  </makefile>
</module>

